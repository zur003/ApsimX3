                    {
                      "$type": "Models.Manager, Models",
                      "Code": "using System;\r\nusing System.Xml.Serialization;\r\nusing Models.Core;\r\nusing Models.PMF;\r\nusing Models.Soils;\r\nusing APSIM.Shared.Utilities;\r\nusing Models.Utilities;\r\n\r\n\r\nnamespace Models\r\n{\r\n    [Serializable]\r\n    public class Script : Model\r\n    {\r\n        // - Links to APSIM models\r\n        [Link] Zone zone;\r\n        [Link] Simulation Simulation;\r\n        [Link] Clock Clock;\r\n        [Link] Fertiliser fertiliser;\r\n        [Link] SoluteManager mySolutes;\r\n        [Link] Soil mySoil;\r\n        [Link] ISummary summary = null;\r\n        [Link] AgPasture.Sward Pasture;\r\n\r\n        //[ScopedLinkByName] Report.Report HarvestReport;\r\n\r\n        [Separator(\"Point to component and true/false variable for the harvesting\")]\r\n        [Description(\"Name of the component that does the reporting       :\")] public string ReportName { get; set; }\r\n\r\n        public double HarvestedWt { get; set; }\r\n        public double IntervalNHA { get; set; }\r\n\r\n\r\n        [XmlIgnore][Units(\"\")] public DateTime[] ResetDates { get; set; }\r\n        [XmlIgnore][Units(\"\")] public DateTime[] ReportDates { get; set; }\r\n\r\n        [XmlIgnore][Units(\"kgN/ha\")] public double HerbageCut { get; set; }\r\n        [XmlIgnore][Units(\"kgN/ha\")] public double HerbageNCut { get; set; }\r\n        [XmlIgnore][Units(\"kgN/ha\")] public double Fixation { get; set; }\r\n        [XmlIgnore][Units(\"kgN/ha\")] public double LeachN { get; set; }\r\n        [XmlIgnore][Units(\"kgN/ha\")] public double Denit { get; set; }\r\n        [XmlIgnore][Units(\"kgN/ha\")] public double DenitN2O { get; set; }\r\n        [XmlIgnore][Units(\"kgN/ha\")] public double Drainage { get; set; }\r\n        [XmlIgnore][Units(\"-\")] public double PropLeached { get; set; }\r\n\r\n        private double[] soilUrea;\r\n        private double[] soilNH4;\r\n        private double[] soilNO3;\r\n        private double urineApplied;\r\n\r\n        [EventSubscribe(\"StartOfSimulation\")]\r\n        private void OnStartOfSimulation(object sender, EventArgs e)\r\n        {\r\n        \t// set the reset and reporting dates\r\n        \tDateTime[] urineDates = (DateTime[])Apsim.Get(zone, \"[AddUrinePatches].Script.DepositionDates\");\r\n        \tResetDates = new DateTime[urineDates.Length + 1];\r\n        \tReportDates = new DateTime[urineDates.Length + 1];\r\n        \tfor (int i = 0; i < urineDates.Length; i++)\r\n        \t{\r\n        \t\tResetDates[i] = urineDates[i].AddDays(-1);  // -1 because this manager is after AddUrinePatches\r\n        \t\tReportDates[i] = urineDates[i].AddYears(2).AddDays(-2); // -2 to capture the data before the reset\r\n        \t\tsummary.WriteMessage(this, \"ReportDate \" + i + \" \" + ReportDates[i]);\r\n        \t}\r\n        \tsoilUrea = new double[mySoil.Depth.Length];\r\n        \tsoilNH4 = new double[mySoil.Depth.Length];\r\n        \tsoilNO3 = new double[mySoil.Depth.Length];\r\n\r\n        \turineApplied = (double)Apsim.Get(zone, \"[AddUrinePatches].Script.DepositionLoad\");\r\n       \t\tsummary.WriteMessage(this, \"DoReportingSensit has found a urineApplied value of \" + urineApplied);\r\n\r\n       \t\tsummary.WriteMessage(this, \"DoReportingSensit has initialised\");\r\n        }\r\n        \r\n        [EventSubscribe(\"DoManagement\")]\r\n        private void OnDoManagement(object sender, EventArgs e)\r\n        {\r\n\t\t\tif (DateUtilities.Equals(ResetDates[0], Clock.Today))\r\n\t\t\t{\r\n\t\t\t\tsoilUrea = mySoil.UreaN;\r\n\t\t\t\tsoilNH4 = mySoil.NH4N;\r\n\t\t\t\tsoilNO3 = mySoil.NO3N;\r\n\t\t\t\tsummary.WriteMessage(this, \"Capturing soil mineral N values for future resetting\");\r\n\t\t\t}\r\n\r\n        \tforeach (DateTime myDate in ResetDates)\r\n        \t{\r\n    \t\t\tif (DateUtilities.Equals(myDate, Clock.Today))\r\n        \t\t{\r\n\t\t\t\t\tmySoil.UreaN = soilUrea;\r\n\t\t\t\t\tmySoil.NH4N = soilNH4;\r\n\t\t\t\t\tmySoil.NO3N = soilNO3;\r\n\t\t\t\t\tsummary.WriteMessage(this, \"Resetting soil mineral N values prior to urine deposition\");\r\n        \t\t}\r\n        \t}\r\n\r\n        \tDateTime[] urineDates = (DateTime[])Apsim.Get(zone, \"[AddUrinePatches].Script.DepositionDates\");\r\n        \tforeach (DateTime myDate in urineDates)\r\n        \t{\r\n    \t\t\tif (DateUtilities.Equals(myDate, Clock.Today))\r\n        \t\t{\r\n\t\t\t\t\tHerbageCut = 0.0;\r\n\t\t\t\t\tHerbageNCut = 0.0;\r\n\t\t\t\t\tFixation = 0.0;\r\n\t\t\t\t\tLeachN = 0.0;\r\n\t\t\t\t\tDenit = 0.0;\r\n\t\t\t\t\tDenitN2O = 0.0;\r\n\t\t\t\t\tDrainage = 0.0;\r\n\t\t\t\t\tPropLeached = 0.0;\r\n\t\t\t\t\tsummary.WriteMessage(this, \"Resetting reporting values\");\r\n        \t\t}\r\n        \t}\r\n        }\r\n\r\n        [EventSubscribe(\"DoManagementCalculations\")]\r\n        private void OnDoManagementCalculations(object sender, EventArgs e)\r\n        {\r\n\t\t\tHerbageCut += Pasture.HarvestedWt;\r\n\t\t\tHerbageNCut += Pasture.HarvestedN;\r\n\t\t\tFixation += Pasture.FixedN;\r\n\t\t\tLeachN += mySoil.SoilWater.LeachUrea + mySoil.SoilWater.LeachNH4 + mySoil.SoilWater.LeachNO3;\r\n\t\t\tfor (int i = 0; i < mySoil.Depth.Length; i++)\r\n\t\t\t{\r\n\t\t\t\tDenit += mySoil.SoilNitrogen.Denitrification[i];\r\n\t\t\t\tDenitN2O += mySoil.SoilNitrogen.n2o_atm[i];\r\n\t\t\t}\r\n\t\t\tDrainage += mySoil.SoilWater.Drainage;\r\n\r\n\t\t\tforeach (DateTime myDate in ReportDates)\r\n        \t{\r\n    \t\t\tif (DateUtilities.Equals(myDate, Clock.Today))\r\n        \t\t{\r\n\t\t\t\t\tif (urineApplied > 0.0)\r\n\t\t\t\t\t{\r\n\t\t\t\t\t\tPropLeached = LeachN / urineApplied;\r\n\t\t\t\t\t\tsummary.WriteMessage(this, \"PropLeached calc first\" + LeachN + \" \" + urineApplied);\r\n\t\t\t\t\t}\r\n\t\t\t\t\telse\r\n\t\t\t\t\t{\r\n\t\t\t\t\t\tPropLeached = 0.0;\r\n\t\t\t\t\t\tsummary.WriteMessage(this, \"PropLeached calc second\" + LeachN + \" \" + urineApplied);\r\n\t\t       \t\t}\r\n\t\t       \t\tModels.Report.Report myReportName = Apsim.Find(this, ReportName) as Models.Report.Report;\r\n\t\t\t\t\tif (myReportName == null)\r\n   \t\t\t\t\t\tthrow new Exception(string.Format(\"Unable to locate report {0}.\", ReportName));\r\n\t\t\t\t\tmyReportName.DoOutput();\r\n        \t\t}\r\n        \t}\r\n        }\r\n    }\r\n}\r\n",
